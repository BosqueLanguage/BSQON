declare namespace Main;

entity Pixel {
    field r: Int;
    field g: Int;
    field b: Int;
    field a: Int;
}


public function main(pix: Pixel): Int { 
	let red = 250i;
	let green = 0i;
	let blue = 250i;
	let alpha = 250i;

	let px = Pixel{red,green,blue,alpha};

	assert px.r !== pix.r;
	assert px.g !== pix.g;
	assert px.b !== pix.b;
	assert px.a !== pix.a;

	return pix.r;
}


