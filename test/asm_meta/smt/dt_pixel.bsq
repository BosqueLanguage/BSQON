declare namespace Main;

entity Pixel {
    field r: Int;
    field g: Int;
    field b: Int;
    field a: Int;
}


public function main(pix: Pixel): Int { 
	let red = 255i;
	let green = 0i;
	let blue = red;
	let alpha = 255i;

	let px = Pixel{red,green,blue,alpha};

	assert px.r !== pix.b;

	return px.r;
}


